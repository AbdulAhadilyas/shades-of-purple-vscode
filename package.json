{
	"name": "shades-of-purple",
	"displayName": "Shades of Purple",
	"description": "âš¡ A professional theme suite with hand-picked & bold ðŸ¦„ shades of purple for your VS Code editor and terminal apps. One of the excellent most downloaded and top rated VSCode Themes on the marketplace. Part of VSCode.pro course.",
	"version": "6.6.0",
	"publisher": "ahmadawais",
	"author": {
		"name": "Ahmad Awais",
		"email": "me@AhmadAwais.com",
		"url": "https://twitter.com/MrAhmadAwais"
	},
	"engines": {
		"vscode": "^1.15.0"
	},
	"icon": "images/icon/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ahmadawais/shades-of-purple-vscode"
	},
	"galleryBanner": {
		"color": "#222145",
		"theme": "dark"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"Shades of Purple",
		"Dark Theme",
		"VSCode Power User",
		"Shade of Purple",
		"Cobalt"
	],
	"main": "./src/extension.js",
	"activationEvents": [
		"*",
		"onCommand:shadesOfPurple.enableGlow",
		"onCommand:shadesOfPurple.whatsNew"
	],
	"contributes": {
		"themes": [
			{
				"label": "Shades of Purple",
				"uiTheme": "vs-dark",
				"path": "./themes/shades-of-purple-color-theme.json"
			}
		],
		"commands": [
			{
				"command": "shadesOfPurple.enableGlow",
				"title": "Shades of Purple: Enable Glow"
			},
			{
				"command": "shadesOfPurple.diableGlow",
				"title": "Shades of Purple: Disable Glow"
			},
			{
				"command": "shadesOfPurple.whatsNew",
				"title": "Shades of Purple: What's New"
			}
		],
		"configuration": {
			"title": "Shades of Purple",
			"properties": {
				"shadesOfPurple.brightness": {
					"type": "number",
					"default": 0.45,
					"description": "Set the brightness value for the glow effect. 0.0 is fully transparent and 1.0 is fully bright"
				},
				"shadesOfPurple.disableGlow": {
					"type": "boolean",
					"default": false,
					"description": "Disable the glow effect, but show Shades of Purple editor chrome updates"
				}
			}
		}
	},
	"files": [
		".vscodeignore",
		"themes/shades-of-purple-color-theme.json",
		"themes",
		"themes/*",
		"src/*",
		"greet/*",
		"images/icon/logo.png",
		"package.json",
		"readme.md",
		"code-of-conduct.md",
		"changelog.md"
	],
	"scripts": {
		"start": "nodemon --exec vsce package",
		"log": "auto-changelog && git add . && git ci -m ' ðŸ“– DOC: Update the changelog' && git push",
		"release": "release-it && npx vsce publish",
		"release-major": "release-it major && npx vsce publish",
		"release-dry": "release-it --dry-run",
		"beforeStage": "auto-changelog"
	},
	"dependencies": {
		"semver": "^7.1.3"
	},
	"__metadata": {
		"id": "431aa1a8-74f4-43d5-a83b-f4960510da5f",
		"publisherDisplayName": "Ahmad Awais âš¡",
		"publisherId": "530c7464-efca-4776-9142-c6f0aeb4084e"
	}
}
